{
  "manifest_version": 3,
  "name": "History Mind Map",
  "version": "1.0.0",
  "description": "Visualize your browsing history as an interactive mind map",
  "permissions": ["history", "tabs", "storage"],
  "action": {
    "default_popup": "index.html",
    "default_title": "History Mind Map"
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}